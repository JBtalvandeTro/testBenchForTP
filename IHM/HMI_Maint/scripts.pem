Option Explicit

Sub switch1Close()

	Dim panel, btn
	Dim symbols, msg
    Set panel = Documents("HMI_Maintenance.ipf").ActiveWindow.Object
    Set btn = panel.ActiveScene.Controls("Switch1Button")
    Set symbols = SymbolsManager("DCDC_BB-AUX_MaintenanceCAN-02")
    Set msg = symbols.Item("MAINT_EquipmentTestRequest").DefaultMultiplexer.Send(1, peSendWait)
    
    msg.VariableValue("relaySwitchId") = 1
    msg.VariableValue("relaySwitchCmd") = 2
    
    msg.Write 0

End Sub 

